#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/domain80/Dev/work/centabit/centabit_v0.5/node_modules/vite/bin/node_modules:/Users/domain80/Dev/work/centabit/centabit_v0.5/node_modules/vite/node_modules:/Users/domain80/Dev/work/centabit/centabit_v0.5/node_modules:/Users/domain80/Dev/work/centabit/node_modules:/Users/domain80/Dev/work/node_modules:/Users/domain80/Dev/node_modules:/Users/domain80/node_modules:/Users/node_modules:/node_modules:/Users/domain80/Dev/work/centabit/centabit_v0.5/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/domain80/Dev/work/centabit/centabit_v0.5/node_modules/vite/bin/node_modules:/Users/domain80/Dev/work/centabit/centabit_v0.5/node_modules/vite/node_modules:/Users/domain80/Dev/work/centabit/centabit_v0.5/node_modules:/Users/domain80/Dev/work/centabit/node_modules:/Users/domain80/Dev/work/node_modules:/Users/domain80/Dev/node_modules:/Users/domain80/node_modules:/Users/node_modules:/node_modules:/Users/domain80/Dev/work/centabit/centabit_v0.5/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
